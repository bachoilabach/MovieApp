{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@models/*": ["model/*"], 
      "@utils/*": ["utils/*"],
      "@components/*": ["components/*"],
      "@services/*": ["services/*"], 
      "@props/*": ["props/*"], 
      "@screens/*": ["navigation/screens/*"],
      "@config/*":["config/*"],
      "@hooks/*": ["hooks/*"],
      "@context/*": ["context/*"]
    }
  }
}
